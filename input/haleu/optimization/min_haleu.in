#Optimization of Scenatio 7 for minimizing HALEU

environment 
  tabular_data
    tabular_data_file = 'min_haleu.dat'
  output_file = 'min_haleu.out'
  write_restart = 'min_haleu.rst'
  #error_file = 'min_haleu.err'

method
  soga
    max_iterations = 5
    max_function_evaluations = 100
    seed = 112822
    population_size = 5
    fitness_type merit_function
    mutation_type offset_normal
    mutation_rate 1.0
    crossover_type two_point 
    crossover_rate 0.0
    replacement_type chc = 10
    #print_each_pop
    linear_equality_constraint_matrix
      1 1 1 0 0 0
    linear_equality_constraint_upper_bounds
      100  
    #log_file = 'mmr_share_lwr.log'

model
  single

variables
  continuous_design = 4
    initial_point 33 33 34 25
    lower_bounds 0 0 0 
    upper_bounds 100 100 100 50
    descriptors 'mmr_share' 'xe100_share' 'voygr_share' 'lwr'
  continuous_state = 1
    descriptors = 'ts'
    initial_state = 721 
  discrete_design_set
    integer = 2
    elements_per_variable 7 8
    elements 41 62 74 78 82 86 90 28 56 84 112 140 151 168 185
    descriptors = 'mmr_burnup' 'xe100_burnup'

interface 
  fork 
    asynchronous 
      evaluation_concurrency = 5
    analysis_drivers = 'python min_haleu_driver.py'
    #results_file = 'mmr_share_lwr_results.dat'

responses 
  objective_functions = 1
    #sense = 'min'
    descriptors 'haleu_swu'
  no_gradients 
  no_hessians
