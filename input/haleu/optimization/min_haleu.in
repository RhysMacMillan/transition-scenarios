#Optimization of Scenatio 7 for minimizing HALEU

environment 
  tabular_data
    tabular_data_file = 'min_haleu.dat'
  output_file = 'min_haleu.out'
  write_restart = 'min_haleu.rst'

method
  #coliny_ea
  soga
    max_iterations = 5
    #max_function_evaluations = 100
    #seed = 112822
    population_size = 5
    #fitness_type merit_function
    #mutation_type offset_normal
    #mutation_rate 1.0
    #crossover_type two_point 
    #crossover_rate 0.0
    #replacement_type chc = 10
    #print_each_pop
    
model
  single

variables
  continuous_design = 2
    initial_point 25 25
    lower_bounds 0 0 
    upper_bounds 100 100
    descriptors 'mmr_share' 'lwr' 

interface 
  fork 
    asynchronous 
      evaluation_concurrency = 5
    analysis_drivers = 'python min_haleu_driver.py'

responses 
  objective_functions = 1
  descriptors 'haleu'
  no_gradients 
  no_hessians
