# One-at-a-time analysis for changing transition start time

environment, 
	tabular_data 
		tabular_data_file = 'mmr_share.dat'
	output_file 'mmr_share.out'
	write_restart 'mmr_share.rst'

method,						
    sampling
        sample_type = lhs
	samples 24000 # 100*parameters*(response+2)
	seed = 112822

variables,					
    active all
    uniform_uncertain = 5
    descriptors   'ts'    'lwr'  'mmr_share' 'mmr_burnup' 'xe100_burnup'   

interface,							
	fork							
	  asynchronous
            evaluation_concurrency =  36
          analysis_drivers = 'python mmr_share_driver.py'
        

responses,				
	descriptors = 'enr_u' 'haleu' 'swu' 'haleu_swu' 'waste' 'feed'
	response_functions = 6
	  scalar_responses = 6
	no_gradients
	no_hessians

