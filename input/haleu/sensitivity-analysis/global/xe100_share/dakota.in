# One-at-a-time analysis for changing transition start time

environment, 
	tabular_data 
		tabular_data_file = 'ts_lwr_xe100.dat'
	output_file 'ts_lwr_xe100.out'
	write_restart 'ts_lwr_xe100.rst'

method,						
    sampling
        sample_type = lhs
	samples 2400 # 100*parameters*(response+2)
	seed = 81622

variables,					
    active all
    uniform_uncertain = 3
    lower_bounds  721      0      0
    upper_bounds  901      50     50 
    descriptors   'ts'    'lwr'  'xe100'   

interface,							
	fork							
	  asynchronous
            evaluation_concurrency =  36
          analysis_drivers = 'python ts_lwr_xe100_driver.py'
        

responses,				
	descriptors = 'enr_u' 'haleu' 'swu' 'haleu_swu' 'waste' 'feed'
	response_functions = 6
	  scalar_responses = 6
	no_gradients
	no_hessians

